
#template
apiVersion: v1
kind: Service
metadata:
  name: my-service
spec:
  selector:
    app: MyApp
  ports:
    - protocol: TCP
      port: 80
      targetPort: 9376
  clusterIP: 10.0.171.239
  type: LoadBalancer #Using a load balancer for the service
status:
  loadBalancer:
    ingress:
    - ip: 192.0.2.127